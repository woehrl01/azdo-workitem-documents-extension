{
  "name": "adzo-embedded-documents",
  "version": "1.0.0",
  "description": "Azure DevOps Embedded Documents",
  "keywords": [
    "extensions",
    "Azure DevOps",
    "Visual Studio Team Services"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./*.vsix",
    "compile": "npm run clean && npm run test && webpack --mode production --config webpack.prod.js",
    "compile:dev": "npm run clean && npm run test && webpack --mode development --config webpack.dev.js",
    "build": "npm run compile",
    "build:dev": "npm run compile:dev",
    "serve": "webpack serve --mode development --config webpack.dev.js",
    "package-extension": "tfx extension create --manifest-globs azure-devops-extension.common.json azure-devops-extension.prod.json src/Code/**/*.json",
    "package-extension:dev": "tfx extension create --manifest-globs azure-devops-extension.common.json azure-devops-extension.dev.json src/Code/**/*.json",
    "publish-extension": "tfx extension publish --manifest-globs azure-devops-extension.common.json azure-devops-extension.prod.json src/Code/**/*.json",
    "publish-extension:dev": "tfx extension publish --manifest-globs azure-devops-extension.common.json azure-devops-extension.dev.json src/Code/**/*.json",
    "test": ""
  },
  "dependencies": {
    "@preact/compat": "^17.0.3",
    "azure-devops-extension-api": "^1.158.0",
    "azure-devops-extension-sdk": "^2.0.11",
    "azure-devops-ui": "^2.167.31",
    "preact": "^10.7.1",
    "uuid": "^8.3.2"
  },
  "override": {
    "azure-devops-extension-api": {
      "version": "$azure-devops-extension-api"
    },
    "azure-devops-extension-sdk": {
      "version": "$azure-devops-extension-sdk"
    },
    "azure-devops-ui": {
      "version": "$azure-devops-ui"
    },
    "uuid": {
      "version": "$uuid"
    },
    "tslib": {
      "version": "$tslib"
    },
    "async": {
      "version": "$async"
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^10.4.4",
    "@types/jest": "^26.0.24",
    "@types/react": "^16.8.25",
    "@types/react-dom": "^16.8.5",
    "async": "^3.2.3",
    "base64-inline-loader": "^2.0.1",
    "compression-webpack-plugin": "^9.2.0",
    "css-loader": "^3.0.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^26.1.0",
    "jest-junit-reporter": "^1.1.0",
    "mini-css-extract-plugin": "^2.6.0",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "sass": "^1.50.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^5.3.1",
    "tfx-cli": "^0.11.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.8",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|ts|tsx|jsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|azure-devops-ui|azure-devops-extension-sdk)/)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/src/__mocks__/styleMock.js"
    },
    "preset": "ts-jest/presets/js-with-babel",
    "testResultsProcessor": "./node_modules/jest-junit-reporter",
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html",
      "cobertura"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.test.json"
      }
    }
  }
}
