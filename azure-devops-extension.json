{
    "manifestVersion": 1,
    "id": "azdo-workitem-documents",
    "publisher": "lukaswoehrl",
    "version": "1.0.1",
    "name": "Work Item Embedded Documents",
    "description": "Display linked documents on an additional page at the work item form for direct viewing.",
    "categories": [
        "Azure Boards"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "logo.png"
    },
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "files": [
        {
            "path": "static",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true
        }
    ]
}